   <children>
      <HBox prefHeight="150.0" prefWidth="248.0">
         <children>
            <ImageView fx:id="friendImage" fitHeight="150.0" fitWidth="129.0" pickOnBounds="true" />
            <Label fx:id="friendName" prefHeight="152.0" prefWidth="223.0" text="FriendName" textFill="WHITE">
               <font>
                  <Font name="Jura Book" size="55.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
